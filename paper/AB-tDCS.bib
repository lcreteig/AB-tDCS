pages = {1656--74},
pmid = {9861716},
title = {{Electrophysiological evidence for a postperceptual locus of suppression during the attentional blink.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/9861716},
volume = {24},
year = {1998}
}
@article{Arasanz2012,
abstract = {Patient and neuroimaging research have provided increasing support for a role of the posterior-lateral cerebellum in cognition, particularly attention. During rapid serial visual presentation, when two targets are presented in close temporal proximity ({\textless}500 ms), accuracy at detecting the second target (T2) suffers. This phenomenon is known as the attentional blink (AB), and in cerebellar lesion patients this effect is exaggerated. Damage to the cerebellum may thus disrupt the use of attentional resources during stimulus processing conditions that are temporally demanding. There are reciprocal connections between the cerebral cortex and the contralateral cerebellum, these connections allow for the possibility that lateralized functions in the cerebral cortex (such as language) remain lateralized in the cerebellum. The purpose of this study was to investigate the temporal characteristics of the cerebellar contribution to the AB and to functionally localize the contribution of the cerebellum to the AB using transcranial magnetic stimulation (TMS). We hypothesized that T2 accuracy would decrease after right cerebellar stimulation when the delay between the first target (T1) and T2 was short (120-400 ms) compared to long (720-960 ms). We used continuous theta burst stimulation (cTBS), a form of TMS, to transiently inhibit a focal population of neurons in the left and right posterior-lateral cerebellum of healthy participants (n = 45). Three groups of participants (n = 15) performed the AB before and after either sham, left, or right cerebellar stimulation. The results of this cTBS study support our hypothesis. During the short delay, participants in the right cTBS group showed a greater AB magnitude compared to both the left and sham cTBS groups (p {\textless} 0.05). No difference in T2 detection was found over long delays. The results provide further support for a cerebellar contribution to an integrated neural network recruited during temporally demanding attention-based tasks.},
annote = {No interaction with lag, tDCS groups differ at short-lags but not long},
archivePrefix = {arXiv},
arxivId = {NIHMS150003},
author = {Arasanz, Carla P. and Staines, W. Richard and Schweizer, Tom A.},
doi = {10.3389/fnbeh.2012.00055},
eprint = {NIHMS150003},
file = {:Users/lcreteig/Documents/Mendeley Desktop/Arasanz, Staines, Schweizer{\_}2012{\_}Frontiers in Behavioral Neuroscience.pdf:pdf},
isbn = {1662-5153},
issn = {1662-5153},
