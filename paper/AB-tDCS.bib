keywords = {attentional blink,cerebellum,cerebellum, cognition, visual attention, theta bur,cognition,theta burst stimulation,visual attention},
number = {August},
pages = {1--6},
pmid = {22936903},
title = {{Isolating a cerebellar contribution to rapid visual attention using transcranial magnetic stimulation}},
