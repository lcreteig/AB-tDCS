keywords = {archive,bids,bids-eeg,brain imaging data structure,datasets,eeg,electroencephalography,formatting,standardization},
title = {{BIDS-EEG: an extension to the Brain Imaging Data Structure (BIDS) Specification for electroencephalography}},
