journal = {Scientometrics},
keywords = {Bias,Competition,Misconduct,Publication,Publish or perish,Research evaluation},
number = {3},
pages = {891--904},
title = {{Negative results are disappearing from most disciplines and countries}},
volume = {90},
year = {2012}
}
